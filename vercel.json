{ 
    "version": 2, 
    "builds": [
        {
            "src": "apiindex.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        { 
            "src": "api/(.*)", "dest": "api/index.js" 
        }
    ],
    "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  }
}